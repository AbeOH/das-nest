{
    "compilerOptions": {
        "outDir": "./src/",
        "noImplicitAny": true, //// Change maybe later to true and check before compiling
        "module": "es6",
        "target": "es5",
        "jsx": "react-jsx",
        "allowJs": true,
        "moduleResolution": "node",
        "noEmitOnError": true,
        "allowSyntheticDefaultImports": true,
        "strict": true /// Change maybe later to true and check before compiling
    }
}
